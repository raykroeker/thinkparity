<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <!-- thinkParity -->
    <context-param>
        <param-name>thinkparity.backup.root</param-name>
        <param-value>${thinkparity.home}/var/backup</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.datasource-driver</param-name>
        <param-value>org.apache.derby.jdbc.ClientDriver</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.datasource-url</param-name>
        <param-value>jdbc:derby://localhost:9001/desdemona</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.datasource-user</param-name>
        <param-value>sa</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.datasource-password</param-name>
        <param-value>sa</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.environment</param-name>
        <param-value>DEVELOPMENT_LOCALHOST</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.mail.transport-host</param-name>
        <param-value>shawmail.cg.shawcable.net</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.mail.transport-port</param-name>
        <param-value>25</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.migrator.logerror.notify</param-name>
        <param-value>false</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.migrator.logerror.notify.to</param-name>
        <param-value>raymond+error@thinkparity.com</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.migrator.logerror.notify.cc</param-name>
        <param-value>raymond+error-cc@thinkparity.com</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.mode</param-name>
        <param-value>DEVELOPMENT</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.product-name</param-name>
        <param-value>DesdemonaProduct</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.release-name</param-name>
        <param-value>DEVELOPMENT</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.stream.root</param-name>
        <param-value>${thinkparity.home}/var/stream</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.stream.bind-host</param-name>
        <param-value>localhost</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.stream-host</param-name>
        <param-value>localhost</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.stream-port</param-name>
        <param-value>20000</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.temp.root</param-name>
        <param-value>${thinkparity.home}/var/tmp</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.queue.notification.bind-host</param-name>
        <param-value>localhost</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.queue.notification-host</param-name>
        <param-value>localhost</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.queue.notification-charset</param-name>
        <param-value>ISO-8859-1</param-value>
    </context-param>
    <context-param>
        <param-name>thinkparity.queue.notification-port</param-name>
        <param-value>30000</param-value>
    </context-param>
    <context-param>
        <param-name>xmpp.domain</param-name>
        <param-value>thinkparity.net</param-value>
    </context-param>
    <!-- thinkParity Web Listener -->
    <listener>
        <listener-class>com.thinkparity.desdemona.web.WebListener</listener-class>
    </listener>
    <!-- thinkParity Web Service Servlet -->
    <servlet>
        <servlet-name>ThinkParityServlet</servlet-name>
        <display-name>ThinkParityServlet</display-name>
        <servlet-class>com.thinkparity.desdemona.web.WebService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ThinkParityServlet</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
</web-app>
